{
  "name": "another-gs-ds-component-library",
  "version": "0.1.2",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "npm run clean",
    "clean": "rimraf dist",
    "build": "cross-env BABEL_ENV=production babel src -d dist --extensions '.ts'",
    "build:esbuild": "node ./scripts/esbuild.js",
    "build:tsc:esout": "tsc --outDir esout",
    "build:cra": "react-scripts build",
    "build:esbuild:watch": "node ./scripts/esbuild.js --watch",
    "build:tsc:esout:watch": "tsc --outDir esout --watch",
    "test": "jest",
    "test:cra": "react-scripts test",
    "eject": "react-scripts eject",
    "check-types": "tsc",
    "storybook": "start-storybook -p 6006 -s public --no-dll",
    "build-storybook": "build-storybook -s public --no-dll --docs",
    "chromatic": "npx chromatic --project-token CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes",
    "release": "auto shipit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "src/**/*.stories.tsx"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "polished": "^3.6.6",
    "prop-types": "^15.7.2",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@storybook/addon-a11y": "^6.0.28",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/node-logger": "^6.0.28",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.28",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "auto": "^10.2.0",
    "babel-loader": "^8.1.0",
    "chokidar-cli": "^2.1.0",
    "chromatic": "^5.3.0",
    "cross-env": "^7.0.2",
    "esbuild": "^0.8.19",
    "eslint": "^7.12.1",
    "prettier": "^2.1.2",
    "react-is": "^17.0.1",
    "react-scripts": "4.0.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5",
    "yargs": "^16.1.1"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "description": "Another implementation of GS Design System",
  "main": "esout/index.js",
  "repository": "git@github.com:davidcui2/cuizhi-gs-design-system.git",
  "author": "Zhihao Cui <davidcui2@me.com>",
  "license": "MIT"
}
